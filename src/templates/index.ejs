<!DOCTYPE html>
<html>
  <head>
    <title>Js Raiz</title>
  </head>
  <body>
    <% menus.forEach(function(menu) { %>
    <div class="cardapio">
      <header>
        <h3><%= menu.title %> - <%= menu.restaurant.name %></h3>
      </header>
      <div class="cardapio-body">
        <ul>
          <% menu.sections.forEach(function(section) { %>
          <li><%= section.title %></li>
          <% }) %>
        </ul>
      </div>
    </div>
    <% }) %>
    <script type="module" src="main.js"></script>
  </body>
</html>

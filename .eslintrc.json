{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": "off",
    "implicit-arrow-linebreak": "off",
    "function-paren-newline": "off",
    "no-unused-vars": "warn"
  }
}
